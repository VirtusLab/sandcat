{
	"name": "Rust",
	"dockerComposeFile": "compose.yml",
	"service": "rust",
	"workspaceFolder": "/workspaces/sandcat",
	"features": {
		"ghcr.io/devcontainers-extra/features/claude-code:1": {}
	},
	"postCreateCommand": "bash scripts/post-create.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"anthropic.claude-code",
				"github.vscode-pull-request-github",
				"rust-lang.rust-analyzer",
				"vadimcn.vscode-lldb"
			]
		}
	}
}
